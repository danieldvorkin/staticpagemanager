<%= render partial: "breadcrumb" %>
<%= render partial: "errors", locals: { errors: @resource.errors } %>

<%= form_for @resource do |f| %>
    <%= f.input :name %>
    <%= f.select :project_id, Project.all.map{|p| [p.name, p.id]}, {}, data: { behavior: "select-box" }, multiple: false %>
    <%= f.input :html %>
    <hr/>
    <%= f.input :active %>
    <%= f.submit %>
<% end %>